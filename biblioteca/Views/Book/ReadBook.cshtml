@model List<biblioteca.Models.ViewModels.ListBooksViewModel>

@{
    ViewBag.Title = "Consultar Libros";
}

<h2>@ViewBag.Title</h2>
<br />

<div class="row">
    <div class="col-md-12">

        <table class="table">
            <tr>
                <th>#</th>
                <th>Titulo</th>
                <th>Autor</th>
                <th>Tipo</th>
                <th>Año</th>
                <th>Editorial</th>
            </tr>

            @foreach (var oElemento in Model)
            {
                <tr>
                    <td>@oElemento.Id</td>
                    <td>@oElemento.Titulo</td>
                    <td>@oElemento.Autor</td>
                    <td>@oElemento.Tipo</td>
                    <td>@oElemento.Anio</td>
                    <td>@oElemento.Editorial</td>
                </tr>
            }

        </table>
    </div>
</div>




